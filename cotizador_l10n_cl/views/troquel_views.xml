<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

	<record id="troquel_cotizador_tree" model="ir.ui.view">
	    <field name="name">cotizador.troquel.tree</field>
	    <field name="model">cotizador.troquel</field>
	    <field name="arch" type="xml">
                <tree editable="top">
                    <field name="name" />
                    <field name="z" />
                    <field name="tipo" />
                    <field name="unidades" />
		</tree>

	    </field>
	</record>

        <record id="troquel_mng_cotizador_action" model="ir.actions.act_window">
	    <field name="name">Troqueles</field>
	    <field name="res_model">cotizador.troquel</field>
	    <field name="view_mode">tree</field>
        </record>

        <menuitem id="troquel_mng_menu" parent="menu_mng_cotizador" action="troquel_mng_cotizador_action" sequence="30" />

    </data>
</odoo>
