<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

	<record id="cotizador_consumo_tree" model="ir.ui.view">
	    <field name="name">cotizador.consumo.tree</field>
	    <field name="model">cotizador.consumo</field>
	    <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="uom_category_id" invisible="1"/> 
                    <field name="cost_currency_id" invisible="1"/> 
                    <field name="product_product_id" />
                    <field name="uom_id" /> 
		    <field name="standard_price" widget="monetary" options="{'currency_field': 'cost_currency_id', 'field_digits': True}"/>
                    <field name="consumo_uom_id" domain="[('category_id','=',uom_category_id)]"/> 
                    <field name="cantidad" /> 
                    <field name="costo_consumo" widget="monetary" options="{'currency_field': 'cost_currency_id', 'field_digits': True}"/> 
                    <field name="incluido_en_ldm" /> 
		</tree>

	    </field>
	</record>

